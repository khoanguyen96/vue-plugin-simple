{
  "name": "{{name}}",
  "version": "0.0.0",
  "description": "{{description}}",
  "author": "{{author}}",
  "license": "MIT",
  "main": "dist/bundle.common.js",
  "module": "dist/bundle.esm.js",
  "browser": "dist/bundle.min.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "purge": "rm -rf dist/",
    "compile": "rollup -c",
    "build": "npm run purge && npm run compile",
    "lint": "eslint src/* test/*",
    "beforePublish": "npm run lint && npm run build",
    "test": "jest",
  },
  "files": [
    "dist/",
    "src/"
  ],
  "dependencies": {
    "vue": "latest"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "camelcase": "^4.1.0",
    "chai": "^4.1.2",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "jest": "^21.2.1",
    "jest-serializer-vue": "^0.3.0",
    "jest-vue": "^0.5.3",
    "lodash.clonedeep": "^4.5.0",
    "rollup": "^0.51.8",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-plugin-vue": "^2.5.2",
    "vue-server-renderer": "^2.5.8",
    "vue-test-utils": "^1.0.0-beta.6"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "setupTestFrameworkScriptFile": "./test/setup.js",
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  }
}
